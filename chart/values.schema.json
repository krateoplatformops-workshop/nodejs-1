{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "NodejsApp Values",
  "type": "object",
  "properties": {
    "replicaCount": {
      "type": "integer",
      "default": 1
    },
    "image": {
      "type": "object",
      "properties": {
        "repository": {
          "type": "string"
        },
        "pullPolicy": {
          "type": "string"
        },
        "tag": {
          "type": "string"
        }
      }
    },
    "service": {
      "type": "object",
      "properties": {
        "type": "string"
        },
        "port": {
          "type": "integer"
        },
        "targetPort": {
          "type": "integer"
        }
      }
    },
    "containerPort": {
      "type": "integer"
    }
  }
}
